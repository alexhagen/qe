\begin_layout Standard
\begin_inset Formula 
\[
\frac{d\dot{m}}{dp}=0
\]

\end_inset


\begin_inset Formula 
\[
\left(\frac{d\dot{m}}{dp}\right)_{cr}=VA\frac{dp}{d\rho}+pA\frac{dV}{d\rho}=0
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
G_{cr}^{2}=\rho^{2}\frac{dp}{d\rho}=-\frac{dp}{d\nu}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
c^{2}=\left(\frac{dp}{d\rho}\right)_{s}
\]

\end_inset


\end_layout

