\begin_layout Paragraph
Low Temp, High Sink Density
\end_layout

\begin_layout Standard
High Sink density causes interstitials to hit sinks before finding vacancies.
\end_layout

\begin_layout Standard
Initially
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
C_{i}=C_{v}=K_{0}t
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\tau_{2}=\frac{1}{K_{is}C_{s}}
\]

\end_inset


\end_layout

\begin_layout Standard
Next 
\begin_inset Formula $C_{i}$
\end_inset

 levels from sinks, 
\begin_inset Formula $C_{v}$
\end_inset

 rises linearly
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{\partial C_{i}}{\partial t}=K_{0}-K_{is}C_{i}C_{s}\approx0
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
C_{i}=\frac{K_{0}}{K_{is}C_{s}}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
C_{v}=K_{0}t
\]

\end_inset


\end_layout

\begin_layout Standard
at time 
\begin_inset Formula $\tau_{4}$
\end_inset

 interstial loss from sinks equals loss to vacancies
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
K_{is}C_{i}C_{s}=K_{iv}C_{i}C_{v}\approx K_{iv}C_{i}K_{0}t
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\tau_{4}=\frac{K_{is}C_{s}}{K_{iv}K_{0}}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
C_{v}=\left(\frac{K_{0}K_{is}C_{s}t}{K_{iv}}\right)^{1/2}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
C_{i}=\left(\frac{K_{0}}{K_{is}K_{iv}C_{s}t}\right)^{1/2}
\]

\end_inset


\end_layout

\begin_layout Standard
Steady state is reached at the same time and concentration formula except
 that 
\begin_inset Formula $C_{s}$
\end_inset

 cannot be ignored
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\tau_{3}=\frac{1}{K_{vs}C_{s}}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
C_{v}=\left(\frac{K_{0}K_{is}}{K_{iv}K_{vs}}+\frac{K_{is}^{2}C_{s}^{2}}{4K_{iv}^{2}}\right)^{1/2}-\frac{K_{is}C_{s}}{2K_{iv}}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
C_{v}=\left(\frac{K_{0}K_{vs}}{K_{iv}K_{is}}+\frac{K_{vs}^{2}C_{s}^{2}}{4K_{iv}^{2}}\right)^{1/2}-\frac{K_{vs}C_{s}}{2K_{iv}}
\]

\end_inset


\end_layout

