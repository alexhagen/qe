\begin_layout Standard
\begin_inset Formula 
\[
Y=sputtering\, yield=\frac{mean\,\#\, of\, emitted\, atoms}{incident\, particles}
\]

\end_inset


\end_layout

\begin_layout Standard
for a medium mass ion in 
\begin_inset Formula $keV$
\end_inset

 energies, typical values for 
\begin_inset Formula $Y$
\end_inset

 are 
\begin_inset Formula $\approx1\rightarrow10$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
Y=\gamma F_{D}(E_{0})
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\gamma=materials\, factor=\frac{4.2}{NU_{0}}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
F_{D}(E_{0})=\left.\frac{dE}{dx}\right|_{nuclear\, processes}=\alpha NS_{n}(E_{0})
\]

\end_inset


\end_layout

\begin_layout Standard
Matsunami Semi-Empirical Sputtering Formula
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
Y_{E}(E)=0.42\frac{\alpha_{s}Q_{s}S_{n}(E)}{U_{0}[1+0.35U_{0}S_{e}(\varepsilon)}\left[1+\left(\frac{E_{th}}{E}\right)^{1/2}\right]^{2.8}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
E_{th}=\begin{cases}
(\frac{4}{3})^{6}\frac{U_{0}}{\Lambda} & M_{1}\geq M_{2}\\
\left(\frac{2M_{1}+2M_{2}}{M_{1}+M_{2}}\right)^{6}\frac{U_{0}}{\Lambda} & M_{1}<M_{2}
\end{cases}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
S_{n}(E)=K_{n}S_{n}(\varepsilon)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
K_{n}=\frac{8.478Z_{1}Z_{2}}{(Z_{1}^{2/3}+Z_{2}^{2/3})^{1/2}}\cdot\frac{M_{1}}{(M_{1}+M_{2})}\left(10^{-15}\:\frac{eV\cdot cm^{2}}{atom}\right)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
S_{n}(\varepsilon)=\frac{3.441\varepsilon^{1/2}ln(\varepsilon+2.718)}{1+6.355\varepsilon^{1/2}+\varepsilon(6.882\varepsilon^{1/2}-1.708)}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\varepsilon=\frac{0.03255}{Z_{1}Z_{2}(Z_{1}^{2/3}+Z_{2}^{2/3})^{1/2}}\cdot\frac{M_{2}}{(M_{1}+M_{2})}E\,\,(eV)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\alpha_{s}=0.10+0.155\left(\frac{M_{2}}{M_{1}}\right)^{0.73}+0.001\left(\frac{M_{2}}{M_{1}}\right)^{1.5}
\]

\end_inset


\end_layout

\begin_layout Paragraph
Steady State Concentration during Ion Implantation
\end_layout

\begin_layout Itemize
Overtime the process of sputtering and implantation comes to equilibrium
 when the implanted ions are sputtered as fast as they are implanted.
\end_layout

\begin_layout Itemize
The distribution of implanted ion density is highest at the surface and
 decreases deeper into the material.
\end_layout

\begin_layout Itemize
if ion A is implanted into material B:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{J_{A}}{J_{B}}=r\frac{N_{B}}{N_{A}}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
r=\frac{probability\, of\, B\, atom\, sputtering}{probability\, of\, A\, atom\, sputtering}
\]

\end_inset


\end_layout

\begin_layout Itemize
Define flux of incident ions as 
\begin_inset Formula $J_{i}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
(J_{A}+J_{B})=YJ_{i}
\]

\end_inset


\end_layout

\begin_layout Standard
at steady state 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
J_{A}=J_{i}+J_{B}=(Y-1)J
\]

\end_inset


\end_layout

\begin_layout Standard
gives 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{(Y-1)J_{i}}{J_{i}}=r\frac{N_{B}}{N_{A}}\therefore\frac{N_{A}}{N_{B}}=\frac{r}{Y-1}
\]

\end_inset


\end_layout

\begin_layout Itemize
Sputtering in Alloys and Compounds: elements in alloys will not be sputtered
 at equal rates because of binding energy and ejection probability.
\end_layout

\begin_layout Itemize
Preferntial Sputtering: for a homogenous sample with componentes A and B,
 surface conenctration 
\begin_inset Formula $N^{s}$
\end_inset

 and bulk concentration 
\begin_inset Formula $N^{B}$
\end_inset

.
\end_layout

\begin_layout Standard
Initial Condition
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{N_{A}^{s}}{N_{B}^{s}}=\frac{N_{A}^{B}}{N_{B}^{B}}
\]

\end_inset


\end_layout

\begin_layout Standard
Partial Yield
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
Y_{A}=\frac{ejected\, A}{incident}
\]

\end_inset


\end_layout

\begin_layout Standard
and same for B.
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{Y_{A}}{Y_{B}}=r\frac{N^{s}A}{N^{s}B}\,\, for\, r\, typically\, between\,0.5\, and\,2
\]

\end_inset


\end_layout

\begin_layout Standard
at 
\begin_inset Formula $t=0$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{Y_{A}(0)}{Y_{B}(0)}=r\frac{N_{A}^{s}(0)}{N_{B}^{s}(0)}=r\frac{N_{A}^{B}}{N_{B}^{B}}
\]

\end_inset


\end_layout

\begin_layout Standard
at 
\begin_inset Formula $t=\infty$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{Y_{A}(\infty)}{Y_{B}(\infty)}=\frac{N_{A}^{B}}{N_{B}^{B}}
\]

\end_inset


\end_layout

\begin_layout Standard
at 
\begin_inset Formula $t=\infty$
\end_inset

, the surface becomes enriched with one element
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{N_{A}^{s}(\infty)}{N_{B}^{s}(\infty)}=\frac{1}{r}\frac{N_{A}^{B}}{N_{B}^{B}}
\]

\end_inset


\end_layout

\begin_layout Standard
High Ion Dose Implantation
\end_layout

\begin_layout Standard
