\begin_layout Section
Cross Sections
\end_layout

\begin_layout Paragraph
Differential Cross Sections
\end_layout

\begin_layout Standard
Generalized Isotropic:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\sigma_{s}(E_{i})=\overset{{\scriptstyle \pi}}{\underset{{\scriptstyle 0}}{\int}}\sigma_{s}(E_{i},\theta)\cdot2\pi\cdot sin\theta d\theta
\]

\end_inset


\end_layout

\begin_layout Standard
Hard Sphere:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\begin{gathered}\sigma(E,T)=\frac{C}{E}\\
C=\pi\frac{M_{1}}{M_{2}}\cdot\frac{Z_{1}^{2}Z_{2}^{2}e^{4}}{1}
\end{gathered}
\]

\end_inset


\end_layout

\begin_layout Standard
Coulomb:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\sigma(E,T)=\frac{C}{ET^{2}}=\pi Z_{1}^{2}Z_{2}^{2}e^{4}\cdot\frac{M_{1}}{M_{2}}\cdot\frac{1}{ET^{2}}
\]

\end_inset


\end_layout

\begin_layout Standard
Inverse Square:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\sigma(E,T)=\frac{C}{E^{1/2}T^{3/2}}=\pi Z_{1}^{2}Z_{2}^{2}e^{4}\cdot\frac{M_{1}}{M_{2}}\cdot\frac{1}{E^{1/2}T^{3/2}}
\]

\end_inset


\end_layout

\begin_layout Standard
Born-Meyer:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\begin{gathered}\sigma(E,T)=\frac{\pi B^{2}}{\Lambda E}\left[ln(\frac{A}{\xi E})\right]^{2}\\
\Lambda=\frac{4M_{1}M_{2}}{(M_{1}+M_{2})}\\
\xi=\frac{M_{2}}{M_{1}+M_{2}}
\end{gathered}
\]

\end_inset


\end_layout

\begin_layout Section
Energy Loss
\end_layout

\begin_layout Paragraph
Production Rate
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
Rate=N\overset{{\scriptstyle \infty}}{\underset{{\scriptstyle 0}}{\int}}\overset{{\scriptstyle \infty}}{\underset{{\scriptstyle 0}}{\int}}\Phi(E)\sigma_{d}(E,T)dEdT
\]

\end_inset


\end_layout

\begin_layout Paragraph
Energy Loss Mechanisms
\end_layout

\begin_layout Standard
Mean Distance between Collisions:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\lambda=\frac{1}{N\sigma}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{dE}{dx}=\frac{\overline{T}}{\lambda}=\frac{\frac{\int T\sigma(E,T)dt}{\int\sigma(E,T)dT}}{\frac{1}{\int\sigma(E,T)dT}}=N\int T\sigma(E,T)dT
\]

\end_inset


\end_layout

\begin_layout Standard
Weighted Average Recoil Spectra: (fraction of recoil ions below a certain
 energy)
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
W(T)=\frac{\overset{T}{\underset{E_{d}}{\int}}T\sigma(E,T)dT}{\overset{\Lambda E}{\underset{E_{d}}{\int}T\sigma(E,T)dT}}=\frac{ln(T)-ln(E_{d})}{ln(\Lambda E)-ln(E_{d})}
\]

\end_inset


\end_layout

\begin_layout Paragraph
Nuclear Stopping
\end_layout

\begin_layout Standard
Using Coulomb potential:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\sigma_{s}(E,T)=\frac{\pi b_{0}^{2}}{4}\left(\frac{\Lambda E}{T^{2}}\right)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
b_{0}=\frac{Z_{1}Z_{2}e^{2}}{\xi E}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\xi=\frac{M_{2}}{M_{1}+M_{2}}
\]

\end_inset


\begin_inset Formula 
\[
\left.\frac{dE}{dx}\right|_{n}=NS_{n}(E)=N\int T\sigma(E,T)dT=N\overset{{\scriptstyle T_{max}}}{\underset{{\scriptstyle T_{min}}}{\int}T\frac{\pi b_{0}^{2}}{4}\left(\frac{\Lambda E}{T^{2}}\right)dT}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
T_{max}=\Lambda E
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
T_{min}=E_{a}=\frac{Z_{1}Z_{2}e^{2}}{a}\left(\frac{M_{1}+M_{2}}{M_{2}}\right)e^{-1}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
a=screening\; radius=\frac{\sqrt{2}\lambda a_{B}}{(Z_{1}^{2/3}+Z_{2}^{2/3})^{1/2}}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\left.\frac{dE}{dx}\right|_{n}=N\pi\frac{Z_{1}^{2}Z_{2}^{2}e^{4}M_{1}}{E_{1}M_{2}}ln\frac{\Lambda E_{1}}{E_{a}}
\]

\end_inset


\end_layout

\begin_layout Standard
Using Inverse Square Potential:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\left.\frac{dE}{dx}\right|_{n}=\frac{\pi^{2}}{4}Na^{2}\Lambda E_{a}
\]

\end_inset


\end_layout

\begin_layout Paragraph
Electronic Stopping
\end_layout

\begin_layout Standard
High Energy:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\sigma(E,T)=\frac{\pi b_{0}^{2}}{4}\cdot\frac{\Lambda E}{T^{2}}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\left.\frac{dE}{dx}\right|_{e}N\pi\frac{Z_{1}^{2}e^{4}M_{1}}{E_{1}M_{e}}ln\frac{4M_{e}E_{1}}{IM_{1}}
\]

\end_inset


\end_layout

\begin_layout Standard
Low Energy (Lindhard):
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\left.\frac{dE}{dx}\right|_{e}=\kappa\sqrt{E_{1}}\;\;\;\;\; for\;0<E(keV)<37Z^{7/3}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\kappa=0.3NZ^{2/3}\frac{ev^{1/2}}{\breve{A}}
\]

\end_inset


\end_layout

\begin_layout Standard
Minimum Ion Energy to release an electron
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
E_{c}=\frac{M_{1}}{4m_{e}}I
\]

\end_inset


\end_layout

\begin_layout Paragraph
Electronic Stopping Power
\end_layout

\begin_layout Standard
Bethe Non-Relativistic
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
E>E_{T}\;\;\;\;\; E_{T}=(24.8keV)A_{1}Z_{1}^{4/3}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{dE}{dx}=2\pi N(Z_{1}^{*})^{2}Z_{2}e^{4}\frac{M_{1}}{E_{1}m_{e}}ln\left(\frac{4m_{e}E_{1}}{IM_{1}}\right)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
Z_{1}^{*}=\frac{Z_{1}^{1/3}\bar{h}}{e^{2}}\sqrt{\frac{2E_{1}}{M_{1}}}
\]

\end_inset


\end_layout

\begin_layout Standard
Medium Energy
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
E_{T}>E>E_{c}\;\;\;\;\; E_{c}=A
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{dE}{dx}=0.4NZ_{1}^{2/3}Z_{2}e^{4}ln\left(\frac{4m_{e}E_{1}}{IM_{1}}\right)
\]

\end_inset


\end_layout

\begin_layout Standard
Low Energy
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
E_{c}>E
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{dE}{dx}=\frac{0.3Z_{1}^{5/6}Z_{2}}{(Z_{1}^{2/3}+Z_{2}^{2/3})^{3/2}}\cdot\frac{N}{\sqrt{M_{1}}}\sqrt{E_{1}}\;\;\;\; in\;\frac{eV}{\breve{A}}
