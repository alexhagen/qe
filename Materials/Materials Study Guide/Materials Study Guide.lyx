\begin_layout Standard
Elastic Scattering causes neutron damage if neutron energy
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
E>\frac{E_{d}}{\Lambda}\; for\;\Lambda=\frac{4A}{(1+A^{2})}
\]

\end_inset


\end_layout

\begin_layout Itemize
below 
\begin_inset Formula $\sim0.1\, MeV$
\end_inset

 elastic scattering is isotropic
\end_layout

\begin_layout Itemize
above 
\begin_inset Formula $\sim0.1\, MeV$
\end_inset

 elastic scattering exhibits a forward bias
\end_layout

\begin_layout Standard
elastic displacement cross section:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\sigma_{d_{el}}(E_{n})=\overset{{\scriptstyle \Lambda E}}{\underset{{\scriptstyle E_{d}}}{\int}}\sigma_{el}(E_{n},E)\nu(E)dE
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\sigma_{el}(E_{n},\theta)=\frac{\sigma_{el}(E_{n})}{4\pi}(1+a_{1}(E_{n})cos\theta)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\left|\frac{a\cdot cos\theta}{dE}\right|=\frac{2}{\Lambda E_{n}}
\]

\end_inset


\end_layout

\begin_layout Paragraph
Inelastic Scattering
\end_layout

\begin_layout Standard
Inelastic scatterin creates recoil nucleus after neutron emission
\end_layout

\begin_layout Itemize
Inelastic scattering occurs when compound nucleus is formed and neutron
 is re-emitted with less energy due to nuclear excitation (
\begin_inset Formula $Q$
\end_inset

).
\end_layout

\begin_layout Itemize
Inelastic scattering is isotropic.
\end_layout

\begin_layout Itemize
The recoil energy after inelastic scattering is given by:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
E=\frac{1}{2}\Lambda E_{n}\left[1-\frac{(1+A)Q}{2AE_{n}}-\left(1-\frac{(1+A)Q}{AE_{n}}\right)^{1/2}cos\theta\right]
\]

\end_inset


\end_layout

\begin_layout Standard
Angular Cross Section:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\sigma_{in}(E_{n},\theta)=\frac{\sigma_{in}(E_{n})}{4\pi}
\]

\end_inset


\end_layout

\begin_layout Standard
Angle to Energy transformation
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\left|\frac{dcos\theta}{dE}\right|_{in}=\frac{2}{\Lambda E_{in}}\left[1-\frac{(1+A)Q}{AE_{n}}\right]^{-1/2}
\]

\end_inset


\end_layout

