\begin_layout Paragraph*
Drift Flux and Heat Conduction Module
\end_layout

\begin_layout LyX-Code
[T,alpha,rho]=heattrans(P,shape);
\end_layout

\begin_layout LyX-Code
T,alpha,rho = [z1,z2,...,zn; T1,T2,...,Tn];
\end_layout

\begin_layout Paragraph*
Cross Section Generation and Homogenization Module
\end_layout

\begin_layout LyX-Code
[Sigma_a,nuSigma_f,Sigma_s,D]=crosssection(T,alpha,rho);
\end_layout

\begin_layout LyX-Code
Sigma_a,nuSigma_f,D = [1,2];
\end_layout

\begin_layout LyX-Code
Sigma_s = [1-1,1-2;2-1,2-2];
\end_layout

\begin_layout Paragraph*
Amplitude Function Module
\end_layout

\begin_layout LyX-Code
[P]=ampfunc(Sigma_a,nuSigma_f,Sigma_s,reactivity);
\end_layout

\begin_layout Paragraph*
Shape Function Module
\end_layout

\begin_layout LyX-Code
[shape]=shapefunc(Sigma_a,nuSigma_f,Sigma_s,D);
\end_layout

\begin_layout LyX-Code
shape = [z1,z2,...,zn; phi1,phi2,...,phin];
\end_layout

